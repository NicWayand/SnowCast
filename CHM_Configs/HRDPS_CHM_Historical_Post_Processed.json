    "parameters":
    {
      "file":"meshes/chro_extent_lowRes.param"
    }
  },
  "parameter_mapping": {
    "landcover":"USGS_treeCover.json"
  }, 
 
  "output":
  {
    "output_dir":"HRDPS_Historical_Post_Processed",
    "file":"SnowCast_stations.json",
